<!--components/mine/mine-cell/index.wxml-->
<view class='main-box' bindtap='itemTapAction' id='{{name}}'>
  <view class='main-container'>
    <view class='left-container'>
      <image class='left-icon' src='{{leftIcon}}'></image>
      <text class='module-name'>{{name}}</text>
    </view>
    <image class='right-icon' src='{{rightIcon}}'></image>
  </view>
</view>
